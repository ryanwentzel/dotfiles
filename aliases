# Shortcuts
alias cls="clear"
alias ll="ls -l"
alias lla="ls -la"
alias lss="ls -1"
alias reload!=". ~/.zshrc"
alias devsite="open -a 'Microsoft Edge' --url 'http://localhost:4000'"
alias cddotfiles="cd '$HOME/Library/Mobile Documents/com~apple~CloudDocs/dotfiles'"
alias byword="open -a Byword"
alias macdown="open -a MacDown"






